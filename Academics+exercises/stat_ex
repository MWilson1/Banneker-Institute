        <td id="L106" class="blob-num js-line-number" data-line-number="106"></td>
        <td id="LC106" class="blob-code blob-code-inner js-file-line"><span class="pl-k">def</span> <span class="pl-en">poly1st</span>(<span class="pl-smi">x_vals</span>, <span class="pl-smi">a_values</span>, <span class="pl-smi">plot</span><span class="pl-k">=</span><span class="pl-c1">False</span>, <span class="pl-smi">sigma</span><span class="pl-k">=</span><span class="pl-c1">0</span>):</td>
      </tr>
      <tr>
        <td id="L107" class="blob-num js-line-number" data-line-number="107"></td>
        <td id="LC107" class="blob-code blob-code-inner js-file-line">    </td>
      </tr>
      <tr>
        <td id="L108" class="blob-num js-line-number" data-line-number="108"></td>
        <td id="LC108" class="blob-code blob-code-inner js-file-line">    x_mat <span class="pl-k">=</span> np.resize(x_vals<span class="pl-k">*</span><span class="pl-c1">1.0</span>,(<span class="pl-c1">len</span>(a_values),<span class="pl-c1">len</span>(x_vals)))</td>
      </tr>
      <tr>
        <td id="L109" class="blob-num js-line-number" data-line-number="109"></td>
        <td id="LC109" class="blob-code blob-code-inner js-file-line">    power_mat <span class="pl-k">=</span> np.resize(np.arange(<span class="pl-c1">len</span>(a_values))<span class="pl-k">*</span><span class="pl-c1">1.0</span>, (<span class="pl-c1">len</span>(x_vals), <span class="pl-c1">len</span>(a_values))).T</td>
      </tr>
      <tr>
        <td id="L110" class="blob-num js-line-number" data-line-number="110"></td>
        <td id="LC110" class="blob-code blob-code-inner js-file-line">    a_mat <span class="pl-k">=</span> np.resize(a_values<span class="pl-k">*</span><span class="pl-c1">1.0</span>,(<span class="pl-c1">len</span>(x_vals),<span class="pl-c1">len</span>(a_values))).T</td>
      </tr>
      <tr>
        <td id="L111" class="blob-num js-line-number" data-line-number="111"></td>
        <td id="LC111" class="blob-code blob-code-inner js-file-line">    mats<span class="pl-k">=</span> x_mat<span class="pl-k">**</span>power_mat<span class="pl-k">*</span>a_mat</td>
      </tr>
      <tr>
        <td id="L112" class="blob-num js-line-number" data-line-number="112"></td>
        <td id="LC112" class="blob-code blob-code-inner js-file-line">    y_vals <span class="pl-k">=</span> np.sum(mats, <span class="pl-smi">axis</span><span class="pl-k">=</span><span class="pl-c1">0</span>) <span class="pl-k">+</span> sigma<span class="pl-k">*</span>rand.randn(<span class="pl-c1">len</span>(x_vals))</td>
      </tr>
      <tr>
        <td id="L113" class="blob-num js-line-number" data-line-number="113"></td>
